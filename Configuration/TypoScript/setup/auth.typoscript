plugin.tx_monkeyhead_api {
  settings {
    auth {
      mapping {
        uid {
          type = int
        }
        username {
          type string
        }
        first_name {
          type = string
        }
        last_name {
          type = string
        }
        email {
          type = string
        }
        image {
          type = images
          properties = uid, title, description, alternative
        }
      }
    }
  }
}
