{
  "name": "t12/monkeyhead",
  "type": "typo3-cms-extension",
  "description": "Read Only TYPO3 JSON frontend",
  "license": ["GPL-2.0-or-later"],
  "version": "0.1.0",
  "authors": [
    {
      "name": "Thomas Hempel",
      "role": "Developer",
      "email": "thomas@t12.online"
    }
  ],
  "require": {
    "typo3/cms-core": "^11.5"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "autoload": {
    "psr-4": {
      "T12\\Monkeyhead\\": "Classes/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web",
      "extension-key": "monkeyhead"
    }
  }
}
